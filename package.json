    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "**/*.{ts,tsx,js,jsx,json,scss,css,md}": [
      "prettier --write"
    ],
    "!**/.next/**": [],
    "!**/out/**": []
